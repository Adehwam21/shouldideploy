  "dependencies": {
    "@vercel/analytics": "^1.0.1",
    "@vercel/og": "^0.5.10",
    "date-fns": "^2.30.0",
    "next": "^13.4.12",
    "prettier-eslint-cli": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "20.4.8",
    "@types/react": "18.2.18",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.13",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.2",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
