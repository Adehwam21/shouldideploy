{
    "name": "shouldideploy",
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/api/slack/(.*)",
            "destination": "/api/slack"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ]
}